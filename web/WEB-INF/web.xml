<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>LoginController</servlet-name>
        <servlet-class>proyecto.com.modelo.controller.LoginController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutController</servlet-name>
        <servlet-class>proyecto.com.modelo.controller.LogoutController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonaEstController</servlet-name>
        <servlet-class>proyecto.com.modelo.controller.PersonaEstController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PersonaController</servlet-name>
        <servlet-class>proyecto.com.modelo.controller.UsuarioController</servlet-class>
    </servlet><servlet-mapping>
        <servlet-name>LoginController</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping><servlet-mapping>
        <servlet-name>LogoutController</servlet-name>
        <url-pattern>/Logout</url-pattern>
    </servlet-mapping><servlet-mapping>
        <servlet-name>PersonaEstController</servlet-name>
        <url-pattern>/PersonaEst</url-pattern>
    </servlet-mapping><servlet-mapping>
        <servlet-name>PersonaController</servlet-name>
        <url-pattern>/Persona</url-pattern>
    </servlet-mapping><session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config></web-app>
